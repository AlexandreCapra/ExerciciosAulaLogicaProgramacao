algoritmo "ProceAlteraOl·Mundo"

var
   c : caractere

procedimento Gerador(var a : caractere)
inicio
      EscrevaL("+-------=======------+")
      EscrevaL("+------Ol· Mundo-----+")
      EscrevaL("+-------=======------+")
      EscrevaL("Digite uma nova mensagem: ")
      Leia(a)


fimProcedimento

inicio

     Gerador(c)
     EscrevaL(c)


fimalgoritmo
